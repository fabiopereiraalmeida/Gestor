	<queryString>
		<![CDATA[SELECT gestor.debito.nome,
	gestor.debito.valor,
	gestor.debito.data_debito,
	gestor.pessoa.nome,
	gestor.entidade.nome,
	gestor.entidade.entidade_id,
	gestor.debito.entidade_id,
	gestor.entidade.id
FROM gestor.debito
	INNER JOIN gestor.entidade ON 
	 gestor.debito.entidade_id = gestor.entidade.id 
	INNER JOIN gestor.pessoa ON 
	 gestor.debito.pessoa_id = gestor.pessoa.id 
WHERE 
	 gestor.debito.data_debito BETWEEN $P{DATA_INICIAL_SQL} AND $P{DATA_FINAL_SQL} 
	 AND gestor.entidade.entidade_id LIKE $P{ENTIDADE_PAI}
	 OR gestor.debito.data_debito BETWEEN $P{DATA_INICIAL_SQL} AND $P{DATA_FINAL_SQL} 
	 AND gestor.debito.entidade_id LIKE $P{ENTIDADE_PAI}]]>
	</queryString>
